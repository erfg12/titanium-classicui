	</Columns>
</Listbox>

<Button item = "Title_ChangeTitlePrefix">
	<ScreenID>Title_ChangeTitlePrefix</ScreenID>
	<!--<Font>3</Font>-->
	<RelativePosition>true</RelativePosition>
	<AutoStretch>true</AutoStretch>
	<TopAnchorToTop>false</TopAnchorToTop>
	<BottomAnchorToTop>false</BottomAnchorToTop>
	<LeftAnchorToLeft>true</LeftAnchorToLeft>
	<RightAnchorToLeft>true</RightAnchorToLeft>
	<TopAnchorOffset>29</TopAnchorOffset>
	<BottomAnchorOffset>5</BottomAnchorOffset>
	<LeftAnchorOffset>10</LeftAnchorOffset>
	<RightAnchorOffset>95</RightAnchorOffset>
	<Style_Transparent>false</Style_Transparent>
	<!--<TooltipReference/>-->
	<Style_Checkbox>false</Style_Checkbox>
	<!--<RadioGroup/>-->
	<Text>Change Title</Text>
	<TextColor>
			<R>255</R>
			<G>255</G>
			<B>255</B>
	</TextColor>
	<ButtonDrawTemplate>
		<Normal>A_BtnNormal</Normal>
		<Pressed>A_BtnPressed</Pressed>
		<Flyby>A_BtnFlyby</Flyby>
		<Disabled>A_BtnDisabled</Disabled>
		<PressedFlyby>A_BtnPressedFlyby</PressedFlyby>
	</ButtonDrawTemplate>
</Button>

