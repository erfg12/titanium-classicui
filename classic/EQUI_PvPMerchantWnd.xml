	<!-- The PvP Merchant Window -->	<Screen item = "PvPMerchantWnd">		<RelativePosition>false</RelativePosition>		<Location>			<X>100</X>			<Y>60</Y>		</Location>		<Size>			<CX>510</CX>			<CY>260</CY>		</Size>		<Style_VScroll>false</Style_VScroll>		<Style_HScroll>false</Style_HScroll>		<Style_Transparent>false</Style_Transparent>		<Text>PvP Merchant</Text>		<DrawTemplate>WDT_Rounded</DrawTemplate>		<Style_Titlebar>true</Style_Titlebar>		<Style_Closebox>true</Style_Closebox>		<Style_Minimizebox>true</Style_Minimizebox>		<Style_Border>true</Style_Border>		<Style_Sizable>true</Style_Sizable>			<Pieces>PvpMerchant_NameLabel</Pieces>			<Pieces>PvpMerchant_ItemList</Pieces>			<Pieces>PvpMerchant_PointsAvailableLabel</Pieces>			<Pieces>PvpMerchant_PointsAvailableValue</Pieces>			<Pieces>PvpMerchant_PointsEverLabel</Pieces>			<Pieces>PvpMerchant_PointsEverValue</Pieces>			<Pieces>PvpMerchant_DoneButton</Pieces>
