>		<BottomAnchorToTop>true</BottomAnchorToTop>		<LeftAnchorToLeft>true</LeftAnchorToLeft>		<RightAnchorToLeft>true</RightAnchorToLeft>		<Text>Points Available</Text>		<TextColor>				<R>255</R>				<G>255</G>				<B>255</B>		</TextColor>		<NoWrap>true</NoWrap>		<AlignCenter>true</AlignCenter>		<AlignRight>false</AlignRight>	</Label>	<Label item = "PvpMerchant_PointsAvailableValue">		<ScreenID>PvpMerchant_PointsAvailableValue</ScreenID>		<RelativePosition>true</RelativePosition>		<AutoStretch>true</AutoStretch>		<LeftAnchorOffset>4</LeftAnchorOffset>		<TopAnchorOffset>54</TopAnchorOffset>		<RightAnchorOffset>140</RightAnchorOffset>		<BottomAnchorOffset>68</BottomAnchorOffset>		<TopAnchorToTop>true</TopAnchorToTop>		<BottomAnchorToTop>true</BottomAnchorToTop>		<LeftAnchorToLeft>true</LeftAnchorToLeft>		<RightAnchorToLeft>true</RightAnchorToLeft>		<Text></Text>		<TextColor>				<R>0</R>				<G>255</G>				<B>0</B>		</TextColor>		<NoWrap>true</NoWrap>		<AlignCenter>true</AlignCenter>		<AlignRight>false</AlignRight>	</Label>
