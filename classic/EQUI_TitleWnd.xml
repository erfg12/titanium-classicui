<!-- Title Prefix info.  This is the normal title that players are used to -->
<Listbox item = "Title_AvailableTitles">
	<ScreenID>Title_AvailableTitles</ScreenID>
	<DrawTemplate>WDT_Inner</DrawTemplate>
	<RelativePosition>true</RelativePosition>
